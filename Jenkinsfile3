pipeline {
    agent any
    
    stages {
        stage('Build and run Nginx container') {
            steps {
                script {
                    // Запуск контейнера Nginx
                    docker build .
                    docker run -d -p 8081:80 --name dimax555/19.05.jenkins-web
                    )
                }
            }
        }
    }
}
